AWb;AACEiB,EAAAA,IAAI,EAAE,IADR;AAEEE,EAAAA,QAAQ,EAAE;AAFZ,CAXa,CAAf;AAiBA,IAAMC,MAAM,GAAG,IAAItB,SAAJ,CAAc;AAC3BkB,EAAAA,MAAM,EAANA;AAD2B,CAAd,CAAf;AAIA,eAAeI,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\n\nVue.use(VueRouter)\n\n/** \n * 异步组件加载处理函数\n * @param {Object} AsycnView 需要加载的组件\n * @return {Object} 返回一个promise对象\n * **/\nconst lazyLoadView = AsycnView => {\n\n  const AsyncHandler = () => ({\n    component: AsycnView,\n    loading: require('@/components/common/RouteLoading'),\n    error: require('@/components/common/RouteError'),\n    delay: 200,\n    timeout: 10000\n  })\n\n  return Promise.resolve({\n    functional: true,\n    render(h, {data, children}){\n      return h(AsyncHandler, data, children)\n    }\n  })\n\n}\n\n\nconst routes = [\n  {\n    path: '/',\n    name: 'Home',\n    component: () => lazyLoadView(import('@/views/Home'))\n  },\n  {\n    path: '/404',\n    name: '404',\n    component: () => lazyLoadView(import('@/components/404'))\n  },\n  {\n    path: '**',\n    redirect: '404'\n  }\n]\n\nconst router = new VueRouter({\n  routes\n})\n\nexport default router\n"]}]}